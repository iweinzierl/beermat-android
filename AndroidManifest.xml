<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="de.inselhome.beermat"
          android:installLocation="preferExternal"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="10" android:maxSdkVersion="17" android:targetSdkVersion="14"/>
    <application android:label="@string/app_name"
                 android:icon="@drawable/beermat_icon"
                 android:theme="@style/Theme.Sherlock.Light.DarkActionBar">
        <activity android:name="Beermat"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".NewBillPosition"
                  android:label="New Bill Position"/>
        <activity android:name=".EditBillPosition"
                  android:label="Edit Bill Position"/>
        <activity android:name=".MyBillList"
                  android:label="My Bill List"/>
        <activity android:name=".BillDetail"
                  android:label="Detail of Bill"/>
    </application>
</manifest>
